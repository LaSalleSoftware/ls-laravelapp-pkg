{
    "name": "lasallesoftware/ls-laravelapp-pkg",
    "description": "LaSalle Software Laravel App Package.",
    "keywords": [
      "lasalle software",
      "south lasalle",
      "bob bloom",
      "package",
      "laravel"
    ],
    "homepage": "https://lasallesoftware.ca",
    "license": "MIT",
    "authors": [
      {
        "name": "Bob Bloom",
        "email": "bob.bloom@lasallesoftware.ca",
        "homepage": "https://LasalleSoftware.ca",
        "role": "Developer"
      }
    ],
    "require": {
      "php": "^7.3|^8.0",
      "fideloper/proxy": "^4.4",
      "fruitcake/laravel-cors": "^2.0",
      "guzzlehttp/guzzle": "^7.0.1",
      "laravel/framework": "^8.12",
      "laravel/tinker": "^2.5"
    },
    "autoload": {
      "psr-4": {
        "Lasallesoftware\\Laravelapp\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Lasallesoftware\\Laravelapp\\Test\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Lasallesoftware\\Laravelapp\\LaravelappServiceProvider"
        ]
      }
    },
    "scripts": {
      "test": "phpunit"
    },
    "suggest": {
    },
    "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }  